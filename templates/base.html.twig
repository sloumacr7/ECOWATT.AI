<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width,initial-scale=1">
		<title>
			{% block title %}ECOWatt.Ai
			{% endblock %}
		</title>

		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			<!--lien css-->
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css">

			<link rel="icon" type="image/png" sizes="16x16" href={{asset("/back_librairies/images/favicon.png")}}>
			<link rel="stylesheet" href={{asset("/back_librairies/vendor/owl-carousel/css/owl.carousel.min.css")}}>
			<link rel="stylesheet" href={{asset("/back_librairies/vendor/owl-carousel/css/owl.theme.default.min.css")}}>
			<link href={{asset("/back_librairies/vendor/jqvmap/css/jqvmap.min.css")}} rel="stylesheet">
			<link href={{asset("/back_librairies/css/style.css")}} rel="stylesheet">
		{% endblock %}


	</head>
	<body>
		<!--*******************
										                            Preloader start
										                    ********************-->
		<div id="preloader">
			<div class="sk-three-bounce">
				<div class="sk-child sk-bounce1"></div>
				<div class="sk-child sk-bounce2"></div>
				<div class="sk-child sk-bounce3"></div>
			</div>
		</div>
		<!--*******************
										                        Preloader end
										                    ********************-->


		<!--**********************************
										                                Main wrapper start
										                    ***********************************-->
		<div id="main-wrapper">
			{{ include ('navbar.html.twig') }}
			<br><br><br><br>
			 {% block body %}{% endblock %}
				<br><br><br><br>
			</div>
			{{ include ('footer.html.twig') }}
		</div>
	</body>

	{% block javascripts %}
		<!--**********************************
										                                Scripts
										                            ***********************************-->
		<!-- Required vendors --><script src={{asset("/back_librairies/vendor/global/global.min.js")}}> </script>
		<script src={{asset("/back_librairies/js/quixnav-init.js")}}></script>
		<script src={{asset("/back_librairies/js/custom.min.js")}}></script>


		<!-- Vectormap -->
		<script src={{asset("/back_librairies/vendor/raphael/raphael.min.js")}}></script>
		<script src={{asset("/back_librairies/vendor/morris/morris.min.js")}}></script>


		<script src={{asset("/back_librairies/vendor/circle-progress/circle-progress.min.js")}}></script>
		<script src={{asset("/back_librairies/vendor/chart.js/Chart.bundle.min.js")}}></script>

		<script src={{asset("/back_librairies/vendor/gaugeJS/dist/gauge.min.js")}}></script>

		<!--  flot-chart js -->
		<script src={{asset("/back_librairies/vendor/flot/jquery.flot.js")}}></script>
		<script src={{asset("/back_librairies/vendor/flot/jquery.flot.resize.js")}}></script>

		<!-- Owl Carousel -->
		<script src={{asset("/back_librairies/vendor/owl-carousel/js/owl.carousel.min.js")}}></script>

		<!-- Counter Up -->
		<script src={{asset("/back_librairies/vendor/jqvmap/js/jquery.vmap.min.js")}}></script>
		<script src={{asset("/back_librairies/vendor/jqvmap/js/jquery.vmap.usa.js")}}></script>
		<script src={{asset("/back_librairies/vendor/jquery.counterup/jquery.counterup.min.js")}}></script>


		<script src={{asset("/back_librairies/js/dashboard/dashboard-1.js")}}></script>
		<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>
		<script>
			// Ensure jQuery is loaded
if (typeof jQuery == 'undefined') {
console.error('jQuery is not loaded!');
} else {
console.log('jQuery is loaded!');
}

// Initialize Bootstrap Datepicker
$('.datepicker').datepicker({format: 'yyyy-mm-dd'});
		</script>
	{% endblock %}
</html>